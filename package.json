{
  "name": "@maany/kitui",
  "version": "1.0.0",
  "description": "A CLI utility to develop DAD UI Kit components",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": {
    "executable_name": "cli.js",
    "kitui": "dist/cli.js",
    "@maany/kitui": "dist/cli.js"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "prebuild:types": "rimraf ./dist",
    "build:types": "tsc -p ./tsconfig.json --outDir build --declaration true && api-extractor run",
    "predocs": "rimraf ./docs",
    "docs": "typedoc src --out docs",
    "clean": "rimraf ./build ./dist ./docs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Mayank Sharma <imptodefeat@gmail.com>",
  "homepage": "https://github.com/maany/kitui",
  "repository": {
    "type": "git",
    "url": "https://github.com/maany/kitui"
  },
  "license": "ISC",
  "files": [
    "dist"
  ],
  "keywords": [
    "typescript",
    "npm",
    "package",
    "boilerplate"
  ],
  "devDependencies": {
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-typescript": "11.1.6",
    "eslint": "8.57.0",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "rollup": "4.12.0",
    "rollup-plugin-dts": "6.1.0",
    "ts-node": "10.9.2",
    "tslib": "2.6.2",
    "typedoc": "0.25.8",
    "typescript": "5.3.3"
  }
}
